@using Entities.Concrete
@model List<Admin>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br /><br />
<table class=" table table-hover">
    <tr>
        <th>Kullanıcı Adı</th>
        <th>Rolü</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.UserName</td>
            <td>@item.Role</td>
        </tr>
    }
</table>
<br />
<a href="/Admin/AddAdmin" class="btn btn-default">Yeni Admin Kişisi Ekle</a>

